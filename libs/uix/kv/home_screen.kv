    source: root.icon
    tooltip_text:"tooltip injected"
<ThirdScreen>:
    id: main_image                                                      
    source:"/Users/rajatmishra/Desktop/chain-link-690503_1920.jpg"     
    canvas:
        Color:                                                               
            rgba:(0,0,0,0.3)
        Line:
            width:2
            points: self.center_x,self.center_y-self.norm_image_size[1] / 2,self.center_x,self.center_y
            dash_length:5
            dash_offset:50 
        Line:
            width:2
            points: self.center_x-self.norm_image_size[0] / 2,self.center_y,self.center_x,self.center_y
            dash_length:5
            dash_offset:50 
        Line:
            width:2
            points: self.center_x-self.norm_image_size[0] / 2,self.center_y,self.center_x+self.norm_image_size[0] / 2,self.center_y
            dash_length:5
            dash_offset:50 
        Line:
            width:2
            points:self.center_x,self.center_y+self.norm_image_size[1] / 2,self.center_x,self.center_y
            dash_length:5
            dash_offset:50 
        Rectangle:                                                      
            id:root.rect_box                                          
            pos:(root.x1, root.y1)                                     
            size:(root.t_x - root.x1, root.t_y - root.y1)


<CustomDropDown>:
    btn:btn.__self__
    dropdown:dropdown.__self__
    MDRectangleFlatIconButton:
        id: btn
        icon: "data/logo/kivy-icon-256.png"
        text: root.text
        theme_text_color: "Custom"
        text_color: 0, 0, 1, 1
        line_color: 1, 0, 1, 1
        icon_color: 1, 0, 0, 1
        on_release: dropdown.open(self)
        size_hint_y: None
        height:'48dp'
        size_hint_x: .8
    CustDropDown:
        id: dropdown
        on_select: root.text = '{}'.format(args[1])
        Button:
            text: 'First Item'
            size_hint_y: None
            height: '48dp'
            on_release: dropdown.select('First Item')
        Button:
            text: 'Third Item'
            size_hint_y: None
            height: '48dp'
            on_release: dropdown.select('Third Item')
