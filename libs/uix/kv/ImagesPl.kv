#:import SweetAlert kivymd_extensions.sweetalert.SweetAlert

<MyTile@SmartTileWithLabel+MDTooltip>
    size_hint_y: None
    tooltip_text: "click to send email"
    height: "240dp"
<MyCard@CheckBox>:
    background_checkbox_normal: "1.png"
    background_checkbox_down:"1.png"
    size:(200, 200)
<ImagesPl>:
    MDBoxLayout:
        orientation: "vertical"
        MDToolbar:
            title: "Images"
            right_action_items: [["home", lambda x: app.root.change_screen("filemanager")],["email", lambda x:app.root.change_screen("home")]]
            left_action_items:[["backburger", lambda x:app.root.change_screen("home")]]
        ScrollView:
            do_scroll_x:False
            GridLayout:
                id:layout_content
                cols: 4
                size_hint_y: None
                row_default_height: '200dp'
                row_force_default: True
                spacing:10
                padding: dp(4), dp(4)
                spacing: dp(4)
                MyButton:
                MyButton:
                MyButton:
                MyButton:
                MyButton: