<MyLabel@ButtonBehavior+MDLabel>:
<SubFolder>
    MDBoxLayout:
        orientation: "vertical"
        MDToolbar:
            title: "Project Details"
            right_action_items: [["home", lambda x: app.root.change_screen("filemanager")]]
            left_action_items:[["details", lambda x: app.root.change_screen("home")]]
        BoxLayout:
            orientation:"vertical"
            GridLayout:
                cols:3
                spacing:60
                padding:100
                MDCard:
                    size_hint:.4,.4
                    Image:
                        source:"1.png"
                        size:"240dp","240dp"
                    MDFlatButton:
                        text:"View Image"
                        on_release:app.root.change_screen("image")
                MDCard:
                    size_hint:.4,.4
                    Image:
                        source:"1.png"
                    MDFlatButton:
                        text:"View Video"
                        on_release:app.root.change_screen("video")
            GridLayout:
                cols:2
            GridLayout:
                cols:2
                
